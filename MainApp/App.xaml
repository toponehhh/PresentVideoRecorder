<Application
    x:Class="PresentVideoRecorder.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:helpers="using:PresentVideoRecorder.Helpers"
    xmlns:converts="using:Microsoft.Toolkit.Uwp.UI.Converters"
    >
    <Application.Resources>
        <ResourceDictionary>
            <helpers:LocalizedStrings x:Key="Localized"/>
            <helpers:ViewModelLocator x:Key="VmLocator"/>
            <converts:BoolNegationConverter x:Key="BoolNegationConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
